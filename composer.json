{
  "name": "knitter/codices",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/Knitter/codices",
  "type": "project",
  "license": "GNU AGPL",
  "support": {
    "issues": "https://github.com/Knitter/codices/issues",
    "source": "https://github.com/Knitter/codices"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "yiisoft/yii2": "~2.0.45",
    "yiisoft/yii2-bootstrap5": "*",
    "yiisoft/yii2-symfonymailer": "^2.0",
    "yiisoft/yii2-redis": "^2.0"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.1.0",
    "yiisoft/yii2-gii": "~2.1.0",
    "yiisoft/yii2-faker": "~2.0.0",
    "codeception/codeception": "^4.0",
    "codeception/verify": "~0.5.0 || ~1.1.0",
    "codeception/specify": "~0.4.6",
    "symfony/browser-kit": ">=2.7 <=4.2.4",
    "codeception/module-filesystem": "^1.0.0",
    "codeception/module-yii2": "^1.0.0",
    "codeception/module-asserts": "^1.0.0",
    "codeception/module-phpbrowser": "^1.0",
    "phing/phing": "3.*@dev",
    "yiisoft/yii2-apidoc": "^3.0"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "composer/package-versions-deprecated": true
    }
  },
  "scripts": {
    "post-install-cmd": [
      "yii\\composer\\Installer::postInstall"
    ],
    "post-create-project-cmd": [
      "yii\\composer\\Installer::postCreateProject",
      "yii\\composer\\Installer::postInstall"
    ]
  },
  "extra": {
    "yii\\composer\\Installer::postCreateProject": {
      "setPermission": [
        {
          "codices/runtime": "0777",
          "console/runtime": "0777",
          "web/assets": "0777",
          "console/yii": "0755"
        }
      ]
    },
    "yii\\composer\\Installer::postInstall": {
      "generateCookieValidationKey": [
        "config/codices/web.php"
      ]
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
